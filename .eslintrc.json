{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": 1,
    "semi": 1,
    "no-console": 1,
    "no-undef": "off",
    "no-restricted-globals": "off",
    "no-unused-vars": 1,
    "react/react-in-jsx-scope":"off",
    "react/prop-types":"off",
    "react/display-name": 1,
    "react/jsx-key": 1
  }
}
